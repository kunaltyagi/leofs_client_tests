{require_otp_vsn, "18|19|20"}.

{deps, [
        {hackney, ".*", {git, "https://github.com/benoitc/hackney.git", {tag, "1.6.0"}}},
        {erlcloud, ".*", {git, "https://github.com/gleber/erlcloud.git", {tag, "0.13.4"}}},
        {leo_commons, ".*", {git, "https://github.com/leo-project/leo_commons.git", {tag, "1.1.4"}}}
       ]}.

{erl_opts, [{d, 'NOTEST'},
            fail_on_warning,
            warn_export_all,
            warn_unused_import,
            warn_untyped_record,
            debug_info]}.
